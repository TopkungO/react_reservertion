generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Profile {
  id        Int      @id @default(autoincrement())
  clerkId   String   @unique
  firstname String
  lastname  String
  email     String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
